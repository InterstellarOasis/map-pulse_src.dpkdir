pulse/spark1
{
	ejector
	{
		particle
		{
	      shader sync gfx/sprites/spark.tga

	      displacement 0 0 0 ~2.0

		velocityType 		static
		velocityDir 		linear
		velocity 		      0 -1 0 ~25
		velocityMagnitude 	150

		accelerationType	      static
		accelerationDir		linear
		acceleration 		0 0 -1 ~25
		accelerationMagnitude 	600

		bounce		.3
		radius		75 3 4
		alpha		0 .5 .0
		rotation	0 0 ~360

		lifeTime 2500

		overdrawProtection
		}


		particle
		{
	      shader sync gfx/sprites/spark.tga

	      displacement 0 0 0 ~2.0

		velocityType 		static
		velocityDir 		linear
		velocity 		      0 -1 0 ~65
		velocityMagnitude 	75

		accelerationType	static
		accelerationDir		linear
		acceleration 		0 0 -1 ~25
		accelerationMagnitude 	300

		bounce		.3
		radius		75 3 4
		alpha		0 .5 .0
		rotation	0 0 ~360

		lifeTime 2500

		overdrawProtection
		}

	delay 0
	count infinite
	period 300 - ~100%
	}
}

