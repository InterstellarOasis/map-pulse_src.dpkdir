pulse/steam1
{
	ejector
	{
		particle
		{
		shader sync textures/arachnid2/mist_s
		displacement 0 0 0 0

		velocityType		static
	
		velocityDir		linear
	
		velocityMagnitude	125
		velocity		3 0 0 ~5

		accelerationType	static
		accelerationDir		linear
		acceleration		0 0 10 ~5
		accelerationMagnitude	50


		radius		0 45 125
		alpha		250 .5 0
		rotation	0 ~360 ~360
		bounce		0.0

		lifeTime 2000
		}
	count infinite
	delay 0
	period 50 - ~0%
	}
}

